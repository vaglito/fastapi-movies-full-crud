{
    "conventionalCommits.scopes": [
        "backend"
    ],
    "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
  },
  "python.isort.args":["--profile", "black"],
}